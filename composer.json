	"type": "wordpress-plugin",
	"keywords": [
		"pdf",
		"epub",
		"mobi"
	],
	"homepage": "https://github.com/soderlind/read-offline",
	"license": [
		"GPL-2.0-only"
	],
	"authors": [
		{
			"name": "Per SÃ¸derlind",
			"role": "Developer",
			"email": "per@soderlind.no",
			"homepage": "https://soderlind.no"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"repositories": {
		"phpepub": {
			"type": "vcs",
			"url": "git@github.com:soderlind/phpepub.git",
			"reference": "master"
		}
	},
	"require": {
		"php": "~7.3",
		"mpdf/mpdf": "8.0.10",
		"soderlind/phpepub": "4.0.8.7",
		"wallabag/php-mobi": "1.1.0"
	}
}